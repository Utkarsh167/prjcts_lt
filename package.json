{
	"name": "node_commutev",
	"version": "1.0.0",
	"description": "Node backend in Hapi.js and TypeScript.",
	"main": "index.js",
	"scripts": {
		"prestart": "tsc",
		"start": "npm run lint && tsc && NODE_ENV=local node ./build/server.js",
		"dev-start": "tsc && NODE_ENV=development pm2 start ./build/server.js --name commutev-node",
		"start-windows": "npm run lint && tsc && set NODE_ENV=local && nodemon --exec ts-node -- server.ts",
		"dev-start-windows": "tsc && set NODE_ENV=development && nodemon --exec ts-node -- server.ts",
		"test": "./node_modules/.bin/mocha --timeout 10000 test/*.ts",
		"lint": "tslint -c tslint.json 'src/**/*.ts'",
		"tslint": "tslint --project tsconfig.json"
	},
	"repository": {
		"type": "git",
		"url": "http://gitlab.appinvent.in/NodeRCC/Backend.git"
	},
	"author": "Appinventiv Technologies",
	"license": "ISC",
	"keywords": [
		"google",
		"maps",
		"distance",
		"matrix",
		"api"
	],
	"dependencies": {
		"@types/mongoose": "^5.3.10",
		"@types/node": "^10.12.19",
		"atob": "^2.1.2",
		"axios": "^0.19.2",
		"bcrypt": "^3.0.6",
		"boom": "^7.3.0",
		"btoa": "^1.2.1",
		"bunyan": "^1.8.12",
		"bunyan-debug-stream": "^2.0.0",
		"bunyan-rotating-file-stream": "^1.6.3",
		"csvtojson": "^2.0.8",
		"debug": "^4.1.1",
		"del": "^3.0.0",
		"dotenv": "^6.2.0",
		"exceljs": "^1.9.0",
		"fcm-node": "^1.4.0",
		"good": "^8.1.2",
		"good-console": "^7.1.0",
		"handlebars": "^4.1.0",
		"hapi": "^18.1.0",
		"hapi-auth-basic": "^5.0.0",
		"hapi-auth-bearer-token": "^6.1.0",
		"hapi-swagger": "^9.4.2",
		"inert": "^5.1.1",
		"joi": "^14.3.1",
		"jsonwebtoken": "^8.3.0",
		"jsonwebtoken-refresh": "^5.7.0",
		"lodash": "^4.17.11",
		"md5": "^2.2.1",
		"module-alias": "^2.1.0",
		"moment": "^2.24.0",
		"mongoose": "^5.4.7",
		"node-schedule": "^1.3.2",
		"nodemailer": "^5.1.1",
		"nodemailer-sendgrid-transport": "^0.2.0",
		"nodemailer-ses-transport": "^1.5.1",
		"randomstring": "^1.1.5",
		"tslint": "^5.16.0",
		"validator": "^10.11.0",
		"vision": "^5.4.4",
		"xlsx": "^0.14.1"
	},
	"devDependencies": {
		"codelyzer": "^4.5.0",
		"ts-node": "^8.0.2",
		"typescript": "^3.4.5",
		"typescript-tools": "^0.3.1"
	},
	"_moduleAliases": {
		"@root": "./build",
		"@utils": "./build/src/utils/",
		"@lib": "./build/src/lib/",
		"@base-dao": "./build/src/baseDao",
		"@config": "./build/src/config",
		"@exceptions": "./build/src/exceptions",
		"@interfaces": "./build/src/interfaces",
		"@plugins": "./build/src/plugins",
		"@routes": "./build/src/routes",
		"@modules": "./build/src/modules"
	}
}
