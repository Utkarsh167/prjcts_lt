{
  "name": "ai-parking",
  "version": "1.0.0",
  "description": "Node backend in Hapi.js and TypeScript.",
  "main": "index.js",
  "scripts": {
    "prestart": "tsc",
    "start": "npm run lint && tsc && set NODE_ENV=local && nodemon --exec ts-node -- server.ts",
    "start-old": "npm run lint && tsc && NODE_ENV=local node ./build/server.js",
    "start-shiv": "npm run lint && tsc && set NODE_ENV=local && nodemon --exec ts-node -- server.ts",
    "dev-start": "tsc && NODE_ENV=development pm2 start ./build/server.js --name Commutev_app",
    "start-windows": "npm run lint && tsc && set NODE_ENV=local && nodemon --exec ts-node -- server.ts",
    "dev-start-windows": "tsc && set NODE_ENV=development && nodemon --exec ts-node -- server.ts",
    "test": "./node_modules/.bin/mocha --timeout 10000 test/*.ts",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "tslint": "tslint --project tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.appinvent.in/fleet-management-app/backend-api.git"
  },
  "author": "Translab Innovations",
  "license": "ISC",
  "keywords": [
    "google",
    "maps",
    "distance",
    "matrix",
    "api"
  ],
